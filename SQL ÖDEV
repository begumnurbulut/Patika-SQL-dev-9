--3) Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi yapalım.

UPDATE Employee 
SET name = 'Rihanna Rodger'
WHERE id = 1;

UPDATE Employee
SET birthday = '2000-01-23'
WHERE id = 42;

UPDATE Employee
SET id = 142
WHERE name = 'Rihanna Rodger';

UPDATE employee
SET email = 'test@gmail.com'
WHERE id = 32;

--4) Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM Employee
WHERE id = 5;

DELETE FROM Employee
WHERE birthday = '1999-03-31';

DELETE FROM Employee
WHERE name = 'Matty Dominy';

DELETE FROM Employee
WHERE id IN (1, 10, 12);

DELETE FROM Employee
WHERE email = 'agabites2@prlog.org';
